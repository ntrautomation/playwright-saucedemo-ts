{
    "compilerOptions": {
      "baseUrl": "",
      "paths": {
        "@objects/*" : ["objects/*"],
        "@pages/*" : ["objects/pages/*"],
        "@helpers/*" : ["objects/helper/*"],
        "@environmentConfig/*" : ["objects/environmentConfig/*"],
        "@data/*" : ["objects/data/*"],
        "@fixtures/*" : ["objects/fixtures/*"]
      },
      "resolveJsonModule": true,
      "moduleResolution": "node",
      "target": "ESNext",
      "lib": ["ESNext", "dom"],
      "types": ["node"]
    },
    "include": ["**/*.ts"]
  }