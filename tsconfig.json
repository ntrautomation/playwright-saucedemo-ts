{
    "compilerOptions": {
      "baseUrl": "",
      "paths": {
        "@e2e/*" : ["e2e/*"],
        "@pages/*" : ["e2e/pages/*"],
        "@helpers/*" : ["e2e/helpers/*"],
        "@environmentConfig/*" : ["e2e/environmentConfig/*"],
        "@data/*" : ["e2e/data/*"],
        "@fixtures/*" : ["e2e/fixtures/*"]
      },
      "resolveJsonModule": true,
      "moduleResolution": "node",
      "target": "ESNext",
      "lib": ["ESNext", "dom"],
      "types": ["node"]
    },
    "include": ["**/*.ts", "tsconfig.json"]
  }